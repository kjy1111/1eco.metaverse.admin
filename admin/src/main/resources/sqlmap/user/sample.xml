<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="grinbi.User">

    <select id="getUser" parameterType="oneEco.model.AdminLoginModel" resultType="oneEco.model.AdminUserModel">
		select	*
		from UserInfo
		where UserId = #{userId}
	</select>

</mapper>
